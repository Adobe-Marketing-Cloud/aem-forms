<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <thumbnail.png/>
    <jcr:content
        cq:designPath="/etc/designs/fd/af/default"
        cq:deviceGroups="/etc/mobile/groups/responsive"
        cq:template="/libs/fd/af/templates/tabbedEnrollmentTemplate"
        jcr:language="en"
        jcr:primaryType="cq:PageContent"
        jcr:title="Address Lookup Sample"
        sling:resourceType="fd/af/components/page/tabbedenrollment">
        <guideContainer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fd/af/components/guideContainer"
            actionType="/libs/fd/af/components/guidesubmittype/store"
            guideCss="guideContainer"
            guideNodeClass="guideContainerNode"
            name="guide1">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/fd/af/layouts/defaultGuideLayout"
                toolbarPosition="Bottom"/>
            <rootPanel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/components/rootPanel"
                guideNodeClass="rootPanelNode"
                name="guideRootPanel"
                panelSetType="Navigable">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/layouts/panel/tabbedPanelLayout"
                    enableLayoutOptimization="{Boolean}true"
                    toolbarPosition="Bottom"/>
                <items jcr:primaryType="nt:unstructured">
                    <guidepanel_879805661044659
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General Information"
                        sling:resourceType="fd/af/components/panel"
                        guideNodeClass="guidePanel"
                        name="GeneralInformation">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/layouts/panel/tabbedPanelLayout"
                            columns="1"
                            enableLayoutOptimization="{Boolean}true"
                            toolbarPosition="Bottom"/>
                        <items
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/layouts/gridFluidLayout">
                            <panel1393766227897
                                jcr:description="Personal Information"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Personal Information"
                                sling:resourceType="fd/af/components/panel"
                                guideNodeClass="guidePanel"
                                name="panel1393766227897">
                                <items
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/layouts/gridFluidLayout">
                                    <guidetextbox
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Name"
                                        sling:resourceType="fd/af/components/guidetextbox"
                                        assistPriority="custom"
                                        guideNodeClass="guideTextBox"
                                        name="textbox"/>
                                    <guideradiobutton
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Marital Status"
                                        sling:resourceType="fd/af/components/guideradiobutton"
                                        _value="0"
                                        alignment="guideFieldHorizontalAlignment"
                                        guideNodeClass="guideRadioButton"
                                        name="radiobutton"
                                        options="[0=Single,1=Married]"/>
                                    </items>
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/layouts/gridFluidLayout"
                                    columns="1"
                                    nonNavigable="true"
                                    toolbarPosition="Bottom"/>
                            </panel1393766227897>
                            <panel1393766190488
                                jcr:description="Address"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Address"
                                sling:resourceType="fd/af/components/panel"
                                guideNodeClass="guidePanel"
                                name="panel1393766190488">
                                <items
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/layouts/gridFluidLayout">
                                    <panel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/components/panel"
                                        guideNodeClass="guidePanel"
                                        name="addressPanel">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/layouts/gridFluidLayout"
                                            columns="2"
                                            nonNavigable="true"
                                            toolbarPosition="Bottom"/>
                                        <items
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/layouts/gridFluidLayout">
                                            <guidetextbox
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="House No."
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                guideNodeClass="guideTextBox"
                                                name="textbox"/>
                                            <guidetextbox_3
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Street"
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                guideNodeClass="guideTextBox"
                                                name="textbox_3"/>
                                            <guidetextbox_2
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="City"
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                guideNodeClass="guideTextBox"
                                                name="city"/>
                                            <guidebutton
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Find City"
                                                sling:resourceType="fd/af/components/guidebutton"
                                                assistPriority="custom"
                                                buttonSize="button-medium"
                                                buttonType="button-default"
                                                clickExp="addressPanel.visible =false; lookup.visible =true;"
                                                css="find_button_align"
                                                guideNodeClass="guideButton"
                                                name="button"
                                                type="button"/>
                                            <guidetextbox_1
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="State"
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                guideNodeClass="guideTextBox"
                                                name="state"/>
                                            <guidetextbox_0
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Country"
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                guideNodeClass="guideTextBox"
                                                name="country"/>
                                        </items>
                                    </panel>
                                    <panel_0
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="fd/af/components/panel"
                                        guideNodeClass="guidePanel"
                                        name="lookup"
                                        visibleExp="false">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/layouts/gridFluidLayout"
                                            nonNavigable="true"
                                            toolbarPosition="Bottom"/>
                                        <items
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="fd/af/layouts/gridFluidLayout">
                                            <guidetextbox
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="City Lookup"
                                                sling:resourceType="fd/af/components/guidetextbox"
                                                assistPriority="custom"
                                                css="widget_addressLookupWidget"
                                                guideNodeClass="guideTextBox"
                                                name="addlookup"/>
                                            <guidebutton
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Ok"
                                                sling:resourceType="fd/af/components/guidebutton"
                                                assistPriority="custom"
                                                buttonSize="button-medium"
                                                buttonType="button-default"
                                                clickExp="window.AdressLookupUtil.addressFill(addlookup, city, state, country); lookup.visible = false; addressPanel.visible = true;"
                                                css="ok_button_align"
                                                guideNodeClass="guideButton"
                                                name="button"
                                                type="button"/>
                                        </items>
                                    </panel_0>
                                </items>
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/layouts/gridFluidLayout"
                                    columns="1"
                                    nonNavigable="true"
                                    toolbarPosition="Bottom"/>
                            </panel1393766190488>
                        </items>
                    </guidepanel_879805661044659>
                    <guidepanel_936167221225552
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section 2"
                        sling:resourceType="fd/af/components/panel"
                        guideNodeClass="guidePanel"
                        name="panel2">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/layouts/gridFluidLayout"
                            columns="1"
                            nonNavigable="true"
                            toolbarPosition="Bottom"/>
                        <items jcr:primaryType="nt:unstructured">
                            <guidecheckbox
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Multiple choice Label"
                                sling:resourceType="fd/af/components/guidecheckbox"
                                guideNodeClass="guideCheckBox"
                                name="checkbox"
                                options="[0=Choice 1,1=Choice 2,2=Choice 3]"/>
                        </items>
                    </guidepanel_936167221225552>
                    <panel1392199234944
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Section 3"
                        sling:resourceType="fd/af/components/panel"
                        css="cert"
                        guideNodeClass="guidePanel"
                        name="panel3">
                        <guidetextdraw
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/components/guidetextdraw"
                            _value="The text will appear here"
                            guideNodeClass="guideTextDraw"
                            name="textdraw"/>
                        <items
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/layouts/gridFluidLayout">
                            <guidetextdraw
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="fd/af/components/guidetextdraw"
                                _value="&lt;p>Description of the section&lt;/p>&#xa;"
                                guideNodeClass="guideTextDraw"
                                name="textdraw"/>
                            <guidescribble
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Signature"
                                sling:resourceType="fd/af/components/guidescribble"
                                guideNodeClass="guideScribble"
                                name="scribble"/>
                        </items>
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="fd/af/layouts/gridFluidLayout"
                            columns="1"
                            nonNavigable="true"
                            toolbarPosition="Bottom"/>
                    </panel1392199234944>
                </items>
            </rootPanel>
            <toolbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/components/toolbar"
                css="bottomToolbar"
                guideNodeClass="guideToolbar"
                name="toolbar">
                <items jcr:primaryType="nt:unstructured">
                    <reset
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Reset"
                        sling:resourceSuperType="fd/af/components/guidebutton"
                        sling:resourceType="fd/af/components/actions/reset"
                        buttonType="button-default"
                        clickExp="window.guideBridge.reset()"
                        guideNodeClass="guideButton"
                        name="reset"
                        type="reset"/>
                    <saveGuideDraft
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Save"
                        sling:resourceSuperType="fd/af/components/guidebutton"
                        sling:resourceType="fd/fp/components/actions/saveGuideDraft"
                        buttonType="button-default"
                        clickExp="handleDraftSave()"
                        guideNodeClass="guideButton"
                        name="saveGuideDraft"
                        type="save"/>
                    <submit
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Submit"
                        sling:resourceSuperType="fd/af/components/guidebutton"
                        sling:resourceType="fd/af/components/actions/submit"
                        buttonType="button-default"
                        clickExp="window.guideBridge.submit()"
                        guideNodeClass="guideButton"
                        name="submit"
                        type="submit"/>
                </items>
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/layouts/toolbar/mobileFixedToolbarLayout"/>
            </toolbar>
        </guideContainer>
        <guideformtitle
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fd/af/components/guideformtitle"
            text="&lt;h1>FORM TITLE&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;sub>(Form Subtitle)&lt;/sub>&lt;/h1>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>Form description&lt;/p>&#xa;"
            textIsRich="true"/>
    </jcr:content>
</jcr:root>
